{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "default",
    "description": "Capability for the main window",
    "windows": ["main", "main-*"],
    "permissions": [
      "core:path:default",
      "core:event:default",
      "core:window:default",
      "core:app:default",
      "core:resources:default",
      "core:menu:default",
      "core:tray:default",
      "core:window:allow-set-title",
      "packets:allow-get-packets-list",
      "packets:allow-get-count",
      "packets:allow-search-packets",
      "packets:allow-get-file-body",
      "packets:allow-get-files-list",
      "packets:allow-get-pdf-page",
      "packets:allow-get-pdf-pages-count",
      "packets:allow-update-sender",
      "packets:allow-get-senders",
      "packets:default",
      "service:allow-clear-dirs",
      "service:allow-delete-packet",
      "service:allow-rescan-packet",
      "service:allow-ws-server-online",
      "settings:allow-delete",
      "settings:allow-get",
      "settings:allow-update",
      "date:allow-get-date-now"
    ]
  }
  