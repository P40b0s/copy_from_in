{
  "name": "packets_copyist_tasker",
  "description": "Копирование пакетов МЭДО",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "serve:tauri": "RUST_BACKTRACE=1 tauri build --debug",
    "serve:native": "RUST_BACKTRACE=1  tauri dev",
    "build:win": "cross-env VITE_NETWORK_PROCESSOR=tauri vite build && tauri build --target x86_64-pc-windows-gnu",
    "build:lin": "cross-env VITE_NETWORK_PROCESSOR=tauri vite build && tauri build"
  },
  "dependencies": {
    "@tauri-apps/api": "1.5.3",
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "1.5.9",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "sass": "1.69.7",
    "typescript": "^5.3.3",
    "vite": "5.0.11",
    "vite-plugin-eslint": "^1.8.1"
  },
  "main": "index.ts",
  "repository": "git@github.com:p40b0s/copy_from_in.git",
  "author": "Aleks_xar",
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "node": true
    }
  }
}
